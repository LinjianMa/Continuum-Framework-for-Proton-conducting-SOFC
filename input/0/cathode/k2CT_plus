FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      Phi_lc;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0]; 

internalField   uniform   14.48425;   //exp(-1.272565/0.075307)*exp(-1*F*(0.8254)/R/T)  

boundaryField
{
    upface
    {
        type            zeroGradient;
    }

    downface
    {
        type            zeroGradient;
    }

    fixedWalls
    {
        type            zeroGradient;
    }

    frontAndBack
    {
        type            empty;
    }
}

