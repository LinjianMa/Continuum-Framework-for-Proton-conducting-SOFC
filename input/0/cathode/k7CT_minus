FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      Phi_lc;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0]; 

internalField   uniform   9.47e7;//0.91616ev//1.89734e6;//1.21063ev 

boundaryField
{
    upface
    {
        type            zeroGradient;
    }

    downface
    {
        type            zeroGradient;
    }

    fixedWalls
    {
        type            zeroGradient;
    }

    frontAndBack
    {
        type            empty;
    }
}

