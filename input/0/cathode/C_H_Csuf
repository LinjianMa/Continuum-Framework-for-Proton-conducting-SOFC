FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      Phi_lc;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform   1870.8; //0.25;//0.0225*83147;

boundaryField
{
    upface
    {
        type            zeroGradient;
    }

    downface
    {
        type            zeroGradient;
    }

    fixedWalls
    {
        type            zeroGradient;
    }

    frontAndBack
    {
        type            empty;
    }
}

